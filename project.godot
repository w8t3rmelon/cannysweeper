; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cannysweeper"
config/description="A Minesweeper clone featuring the Canny Cat and its evil counterpart"
config/version="0.1"
run/main_scene="res://Scenes/Main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://Art/Canny.png"

[autoload]

SND="*res://Scripts/Managers/SoundSystem.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=640
window/subwindows/embed_subwindows=false

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Art/": "red",
"res://ArtSources/": "orange",
"res://DifficultyPresets/": "green",
"res://Fonts/": "gray",
"res://Music/": "blue",
"res://Scenes/": "purple",
"res://Scripts/": "blue",
"res://Sounds/": "teal"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
